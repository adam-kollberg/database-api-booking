    "productgallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "date": {
      "type": "datetime"
    },
    "teraphist": {
      "model": "teraphist"
    },
    "bookings": {
      "via": "product",
      "collection": "booking"
    }
